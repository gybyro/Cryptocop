{
  "MessariApi": {
    "BaseUrl": "https://data.messari.io/api/v2"
  },
  "JwtSettings": {
    "Issuer": "CryptoCopAPI",
    "Audience": "MyApiUsers",
    "SecretKey": "ThisIsAReallyLongSecretKeyForJwtToken123!",
    "TokenLifetimeMinutes": 60
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=MyApiDb;Trusted_Connection=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
 